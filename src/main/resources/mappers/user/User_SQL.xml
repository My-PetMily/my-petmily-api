<mapper namespace="com.family.mypetmily.user.mapper.UserMapper">

	<select id="selectCountNickname" parameterType="String" resultType="int">
		SELECT
			COUNT(*)
		FROM user
		WHERE
			nickname = #{nickname}
	</select>

</mapper>